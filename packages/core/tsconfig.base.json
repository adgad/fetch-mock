{
    "compilerOptions": {
        "inlineSourceMap": false,
        "listEmittedFiles": false,
        "listFiles": false,
        "pretty": true,
        "traceResolution": false,
        "rootDir": "src",
        "lib": [
            "es2021",
            "dom",
            "dom.iterable"
        ],
        "moduleResolution": "nodenext",
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "strictFunctionTypes": true,
        "downlevelIteration": true,
        "forceConsistentCasingInFileNames": true,
        "types": [],
        "declaration": true,
        "removeComments": true
    },
    "compileOnSave": false,
    "exclude": ["node_modules", "src/__tests__"],
    "include": ["src"]
}

















